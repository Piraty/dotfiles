[include]
	path = ~/.gitconfig.user
[core]
	editor = vim
[alias]
	s = status
	a = add
	c = commit
	ch = checkout
	d = diff
	l = log
	rebaseUpstream = pull upstream master --rebase --autostash
	graph = log --graph --decorate --oneline
	find = log -i --name-status --grep
[gui]
	warndetachedcommit = true
	spellingdictionary = none
	tabsize = 4
[sendemail]
	annotate = yes
	suppresscc = self
